<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <filter>
        <filter-name>Encoding</filter-name>
        <filter-class>top.cyc.filter.Encoding</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>Encoding</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>GetQRCode</servlet-name>
        <servlet-class>top.cyc.servlet.qrCode.GetQRCode</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetQRCode</servlet-name>
        <url-pattern>/qrcode/get</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>top.cyc.servlet.login.login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>top.cyc.servlet.login.register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>insertMeeting</servlet-name>
        <servlet-class>top.cyc.servlet.meeting.Insert</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>insertMeeting</servlet-name>
        <url-pattern>/meeting/insert</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>getMeetingListByOrganizer</servlet-name>
        <servlet-class>top.cyc.servlet.meeting.GetByOrganizer</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getMeetingListByOrganizer</servlet-name>
        <url-pattern>/meeting/getByOrganizer</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>autoLogin</servlet-name>
        <servlet-class>top.cyc.servlet.login.autoLogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>autoLogin</servlet-name>
        <url-pattern>/login/autologin</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>getMeetingByID</servlet-name>
        <servlet-class>top.cyc.servlet.meeting.GetById</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getMeetingByID</servlet-name>
        <url-pattern>/meeting/getbyid</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>getAttendeeList</servlet-name>
        <servlet-class>top.cyc.servlet.attendee.GetForList</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getAttendeeList</servlet-name>
        <url-pattern>/attendee/get</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>attend</servlet-name>
        <servlet-class>top.cyc.servlet.attendee.Attend</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>attend</servlet-name>
        <url-pattern>/attendee/attend</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>getOpenID</servlet-name>
        <servlet-class>top.cyc.servlet.login.GetOpenId</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getOpenID</servlet-name>
        <url-pattern>/login/getopenid</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>attendeestatisticsByMeeting</servlet-name>
        <servlet-class>top.cyc.servlet.attendee.Statistics</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>attendeestatisticsByMeeting</servlet-name>
        <url-pattern>/attendee/statisticsbymeeting</url-pattern>
    </servlet-mapping>
</web-app>
