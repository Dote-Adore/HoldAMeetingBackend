<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <filter>
        <filter-name>Encoding</filter-name>
        <filter-class>top.cyc.filter.Encoding</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>Encoding</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>GetQRCode</servlet-name>
        <servlet-class>top.cyc.servlet.qrCode.GetQRCode</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetQRCode</servlet-name>
        <url-pattern>/qrcode/get</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>top.cyc.servlet.login.login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GetDepartmentList</servlet-name>
        <servlet-class>top.cyc.servlet.department.GetDepartmentList</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetDepartmentList</servlet-name>
        <url-pattern>/department/getforlist</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>top.cyc.servlet.login.register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
</web-app>
